# =============================================================================
# 🎯 USAGE EXAMPLES
# =============================================================================

# Basic Integration (No API keys needed):
# cd integrations/basic && python main.py demo

# Enhanced Integration (Advanced web scraping):
# cd integrations/basic && python src/enhanced_asset_scraper.py

# Universal Integration (Comprehensive asset management):
# cd integrations/basic && python src/universal_asset_scraper.py

# Phase 1 (Free & Simple):
# cd integrations/phase1 && python main.py setup

# Phase 2 (Free with Registration):
# export SKETCHFAB_API_KEY="your_key"
# export CLARA_IO_TOKEN="your_token"
# cd integrations/phase2 && python main.py setup

# Phase 3 (Game Development):
# export UNITY_ID="your_unity_id"
# export ADOBE_ID="your_adobe_id"
# cd integrations/phase3 && python main.py setup

# Phase 4 (Professional Marketplaces):
# export CGTRADER_API_KEY="your_key"
# export TURBOSQUID_API_KEY="your_key"
# export EPIC_ID="your_epic_id"
# cd integrations/phase4 && python main.py setup

# Advanced Features:
# cd integrations/advanced && python main.py demo

