# =============================================================================
# 🤖 ADVANCED FEATURES CONFIGURATION
# =============================================================================
# AI recommendations, batch processing, and custom platform integration

# AI Recommendation System
AI_MODEL_PATH=models/asset_recommender.pkl
AI_LEARNING_RATE=0.01
AI_SIMILARITY_THRESHOLD=0.7
AI_MAX_RECOMMENDATIONS=10
AI_FEATURE_WEIGHTS='{"visual": 0.3, "technical": 0.25, "content": 0.2, "quality": 0.15, "usage": 0.1}'

# Batch Processing
BATCH_MAX_WORKERS=4
BATCH_CHUNK_SIZE=10
BATCH_TIMEOUT=300
BATCH_RETRY_ATTEMPTS=3
BATCH_MEMORY_LIMIT=2GB
BATCH_PARALLEL_PROCESSING=true

# Custom Platform Integration
CUSTOM_PLATFORM_TEMPLATE_DIR=templates
CUSTOM_PLATFORM_PLUGIN_DIR=plugins
CUSTOM_PLATFORM_VALIDATION=true
CUSTOM_PLATFORM_AUTO_DISCOVERY=true

# =============================================================================
# 🔧 BLENDER MCP CONFIGURATION
# =============================================================================
# Blender MCP server settings for direct Blender integration

BLENDER_MCP_HOST=localhost
BLENDER_MCP_PORT=8000
BLENDER_MCP_DEBUG=false
BLENDER_MCP_LOG_LEVEL=INFO
BLENDER_MCP_TIMEOUT=30
BLENDER_MCP_RETRY_ATTEMPTS=3

# =============================================================================
# ⚙️ GENERAL SYSTEM CONFIGURATION
# =============================================================================
# System-wide settings for asset management

# Asset Storage
ASSET_BASE_DIR=~/3d_assets
ASSET_TEMP_DIR=/tmp/3d_assets
ASSET_CACHE_SIZE=100MB
ASSET_CACHE_TTL=3600
ASSET_DOWNLOAD_DIR=~/3d_assets/downloads
ASSET_IMPORT_DIR=~/3d_assets/imported

# Logging
LOG_LEVEL=INFO
LOG_FILE=logs/integration.log
LOG_MAX_SIZE=50MB
LOG_BACKUP_COUNT=10
LOG_FORMAT='%(asctime)s - %(name)s - %(levelname)s - %(message)s'

# Performance
MAX_CONCURRENT_DOWNLOADS=5
DOWNLOAD_TIMEOUT=300
REQUEST_RATE_LIMIT=60
REQUEST_BURST_LIMIT=10
CONNECTION_POOL_SIZE=10
CONNECTION_TIMEOUT=30

# Security
ENCRYPT_API_KEYS=true
AUDIT_LOGGING=true
SECURE_TEMP_FILES=true
API_KEY_ROTATION_DAYS=90
SECURE_DOWNLOAD=true

# =============================================================================
# 🧪 DEVELOPMENT AND TESTING
# =============================================================================
# Settings for development and testing environments

# Development Mode
DEV_MODE=false
DEBUG_MODE=false
SIMULATION_MODE=false
MOCK_API_RESPONSES=false
VERBOSE_LOGGING=false

# Testing
TEST_MODE=false
TEST_DATA_DIR=test_data
TEST_TIMEOUT=30
TEST_RETRY_ATTEMPTS=3
TEST_MOCK_RESPONSES=true

# =============================================================================
# 📊 PLATFORM-SPECIFIC SETTINGS
# =============================================================================
# Rate limiting and platform-specific configurations

# Rate Limiting (requests per minute)
SKETCHFAB_RATE_LIMIT=1000
CLARA_IO_RATE_LIMIT=2000
UNITY_RATE_LIMIT=2000
MIXAMO_RATE_LIMIT=1000
CGTRADER_RATE_LIMIT=500
TURBOSQUID_RATE_LIMIT=300
UNREAL_RATE_LIMIT=200
GITHUB_RATE_LIMIT=5000
STEAM_RATE_LIMIT=1000
ROBLOX_RATE_LIMIT=500
BEHANCE_RATE_LIMIT=1000
DRIBBBLE_RATE_LIMIT=500
ARTSTATION_RATE_LIMIT=1000
AUTODESK_RATE_LIMIT=1000
SIEMENS_RATE_LIMIT=500
PTC_RATE_LIMIT=500

# Download Settings
DEFAULT_DOWNLOAD_FORMAT=fbx
SUPPORTED_FORMATS=blend,fbx,obj,dae,gltf,glb,3ds,max,ma,mb,blend,stl,3mf
MAX_FILE_SIZE=500MB
DOWNLOAD_CHUNK_SIZE=8192
DOWNLOAD_RETRY_ATTEMPTS=3
DOWNLOAD_TIMEOUT=300

# Import Settings
BLENDER_SCALE_FACTOR=1.0
BLENDER_APPLY_SCALE=true
BLENDER_BAKE_ANIMATIONS=true
BLENDER_OPTIMIZE_MESHES=true
BLENDER_CLEANUP_MESHES=true
BLENDER_APPLY_MODIFIERS=true

# Export Settings
EXPORT_QUALITY=high
EXPORT_TEXTURE_RESOLUTION=2048
EXPORT_POLYGON_LIMIT=50000
EXPORT_MATERIAL_LIMIT=10
EXPORT_ANIMATION_LIMIT=1000

# =============================================================================
# 📈 ANALYTICS AND MONITORING
# =============================================================================
# Analytics and monitoring configuration

# Analytics
ANALYTICS_ENABLED=true
TRACK_USAGE_STATS=true
TRACK_PERFORMANCE_METRICS=true
TRACK_USER_BEHAVIOR=true
TRACK_ASSET_TRENDS=true
ANALYTICS_RETENTION_DAYS=365

# Monitoring
MONITORING_ENABLED=true
HEALTH_CHECK_INTERVAL=60
PERFORMANCE_MONITORING=true
ERROR_TRACKING=true
UPTIME_MONITORING=true

# Reporting
DAILY_REPORTS=true
WEEKLY_REPORTS=true
MONTHLY_REPORTS=true
CUSTOM_REPORTS=true
REPORT_EMAIL=admin@yourdomain.com

# =============================================================================
# 🔄 CACHING AND OPTIMIZATION
# =============================================================================
# Caching and performance optimization

# Caching
CACHE_ENABLED=true
CACHE_BACKEND=memory
CACHE_SIZE=100MB
CACHE_TTL=3600
CACHE_COMPRESSION=true
CACHE_PERSISTENCE=true

# Optimization
LAZY_LOADING=true
CONNECTION_POOLING=true
REQUEST_BATCHING=true
PARALLEL_PROCESSING=true
MEMORY_OPTIMIZATION=true

# =============================================================================
# 🔐 AUTHENTICATION AND SECURITY
# =============================================================================
# Authentication and security settings

# OAuth Settings
OAUTH_REDIRECT_URI=http://localhost:8080/callback
OAUTH_STATE_LENGTH=32
OAUTH_TOKEN_EXPIRY=3600
OAUTH_REFRESH_THRESHOLD=300

# API Key Management
API_KEY_ENCRYPTION=true
API_KEY_ROTATION=true
API_KEY_BACKUP=true
API_KEY_MONITORING=true

# Security Headers
SECURITY_HEADERS=true
CORS_ENABLED=true
CSRF_PROTECTION=true
RATE_LIMITING=true

# =============================================================================
# 🌐 NETWORK AND CONNECTIVITY
# =============================================================================
# Network and connectivity settings

# Proxy Settings
