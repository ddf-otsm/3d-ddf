PROXY_HOST=
PROXY_PORT=
PROXY_USERNAME=
PROXY_PASSWORD=

# SSL/TLS Settings
SSL_VERIFY=true
SSL_CERT_PATH=
SSL_KEY_PATH=
TLS_VERSION=1.2

# Connection Settings
CONNECTION_TIMEOUT=30
READ_TIMEOUT=60
WRITE_TIMEOUT=60
KEEP_ALIVE=true

